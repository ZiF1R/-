#include <iostream>

using namespace std;

class Fraction
{
private:
	int m_numerator;
	int m_denominator;

public:
	Fraction()
	{
		m_numerator;
		m_denominator;
	}

	int getNumerator() { return m_numerator; }
	int getDenominator() { return m_denominator; }
  
	int setValue(int numerator, int denominator)
	{ 
		m_numerator = numerator;
		m_denominator = denominator;
	}
};

int main()
{
	Fraction drob;
	drob.setValue(2, 4);
	cout << drob.getNumerator() << "/" << drob.getDenominator() << "\n";

	system("pause");
	return 0;
}

//Output: 2/4
